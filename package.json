{
  "name": "usersgo",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "vite build --outDir ./dist/public/admin-spa",
    "wc_build": "rollup -c --environment BUILD:deploy",
    "wc_build_styles": "npx tailwindcss -c ./tailwind.config.cjs -i ./src/common/input.css -o ./dist/public/static/tailwin.css",
    "dev": "vite build --outDir ./../api/public/admin-spa --watch",
    "wc_dev": "rollup -c -w",
    "wc_dev_styles": "npx tailwindcss -c ./tailwind.config.cjs -w -i ./src/common/input.css -o ./../api/public/static/tailwin.css",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-node-resolve": "^13.2.0",
    "@rollup/plugin-typescript": "^8.3.1",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.30",
    "@tsconfig/svelte": "^2.0.1",
    "@types/sanitize-html": "^2.6.2",
    "autoprefixer": "^10.4.2",
    "daisyui": "2.14.1",
    "postcss": "^8.4.5",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.7",
    "svelte-icons": "^2.1.0",
    "svelte-preprocess": "^4.9.8",
    "svelte-quill": "^1.2.0",
    "svelte-spa-router": "^3.2.0",
    "tailwindcss": "^3.0.17",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4",
    "vite": "^2.7.2"
  },
  "dependencies": {
    "svelte-carousel": "^1.0.15"
  }
}